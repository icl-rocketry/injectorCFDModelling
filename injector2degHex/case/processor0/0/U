FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            pressureInletOutletVelocity;
        value           nonuniform List<vector> 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            noSlip;
    }
    sides
    {
        type            symmetry;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


